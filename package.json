  "dependencies": {
    "@fortawesome/fontawesome-free-solid": "^5.0.10",
    "bootstrap": "^4.0.0"
  },
  "devDependencies": {
    "lite-server": "^2.4.0",
    "node-sass": "^4.11.0",
    "onchange": "^5.2.0",
    "parallelshell": "^3.0.2"
  },
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'",
    "clean":"rimraf dist",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/*/newBottstrap-4/dist/fonts"
  },
